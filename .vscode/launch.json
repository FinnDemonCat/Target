	"version": "0.2.0",
	"configurations": [
		{
			"name": "Dart: JIT (Debug)",
			"type": "dart",
			"request": "launch",
			"program": "bin/main.dart",
			"preLaunchTask": "CMake: debug",
			"console": "externalTerminal"
		},
		{
            "name": "Dart: AOT (Run)",
            "type": "cppvsdbg",
			"cwd": "${workspaceFolder}/build",
            "request": "launch",
            "program": "${workspaceFolder}/build/TargetEngine.exe", 
            "preLaunchTask": "CMake: build",
			"console": "externalTerminal"
        }
	]
}